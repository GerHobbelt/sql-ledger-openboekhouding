<?xml version="1.0" encoding="utf-8"?>
<html>
<head>
  <title></title>
</head>
<body>
  <sql-ledger-root></sql-ledger-root>
  <head>
  <title>PAYSLIP</title>
  <link rel="stylesheet" href="<%templates%>/<%language_code%>/sqledger.css" />
</head>
<body>
  <table width="100%">

  <%include letterhead.html%>

    <tr>
       
        <h4>PAYSLIP</h4>
       
    </tr>

    <tr>
       
        <%source%>
       
    </tr>

    <tr>
        
    </tr>

    <tr>
       
        <%text_amount%> \dotfill 
       
       
        <%decimal%>/100
       
    </tr>
  </table>
  
  <table width="100%">
    <tr>
        
      
       
        <%datepaid%>
       
       
       <%net%>
       
    </tr>
  </table>
    
  
  <table width="100%">
    <tr>
       
        <%name%>
        <br>
        <%address1%>
        <br>
        <%address2%>
        <br>
        <%city%>
<%if state%>
        , <%state%>
<%end state%>
        <%zipcode%>
        <br>
        <%country%>
       
    </tr>
    
    <tr>
       
        <%memo%>
       
    </tr>
    
    <tr>
       
        <%company%>
       
    </tr>
  </table>
  
  <table width="100%">
    <tr>
       
        <%name%>
       
       
       <%datepaid%>
       
       
        For Pay Period: <%dateto%>
       
       
        <%source%>
       
    </tr>
  </table>
  
  <table width="100%">
    <tr>
       
        Regular
       
      <!-- \dotfill -->
       
          <%regularpay%>
       
    </tr>
    <tr>
       
        Overtime
       
      <!-- \dotfill -->
       
         <%overtimepay%>
       
    </tr>
    <tr>
       
        Travel
       
      <!-- \dotfill -->
       
         <%travelpay%>
       
    </tr>
    <tr>
       
        Stat Holiday
       
      <!-- \dotfill -->
       
         <%holidaypay%>
       
    </tr>
    <tr>
       
        <hr>
       
    </tr>
    <tr>
       
        Gross
       
      <!-- \dotfill -->
       
         <%gross%>
       
    </tr>
  </table>
  
  <table width="100%">
<%foreach deduction%>
    <tr>
       
        <%deduction%>
       
      <!-- \dotfill -->
       
        <%amount%>
       
    </tr>
<%end deduction%>

    <tr>
       
        <hr>
       
    </tr>
    
    <tr>
       
          Withheld
       
      <!-- \dotfill -->
       
        <%withheld%>
       
    </tr>
  </table>
  
  <table width="100%">
    <tr>
       
        Gross
       
      <!-- \dotfill -->
       
         <%gross%>
       
    </tr>
    <tr>
       
        Withheld
       
      <!-- \dotfill -->
       
         <%withheld%>
       
    </tr>
    <tr>
       
        Benefits
       
      <!-- \dotfill -->
       
         <%benefits%>
       
    </tr>
    <tr>
       
        <hr>
       
    </tr>
    <tr>
       
        Net
       
      <!-- \dotfill -->
       
         <%net%>
       
    </tr>
  </table>
  
  <table width="100%">
    <tr>
       
        Regular:
       
       
        <%regular%>
       
       
        <%regularrate%>
       
    </tr>
    <tr>
       
        Overtime:
       
       
        <%overtime%>
       
       
        <%overtimerate%>
       
    </tr>
    <tr>
       
        Travel:
       
       
        <%travel%>
       
       
        <%travelrate%>
       
    </tr>
    <tr>
       
        Stat Holiday:
       
       
        <%holiday%>
       
      
        
    </tr>
  </table>
</body>
</html>
