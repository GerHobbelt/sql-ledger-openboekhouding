<?xml version="1.0" encoding="utf-8"?>
<html>
<head>
  <title></title>
</head>
<body>
  <sql-ledger-root></sql-ledger-root>
  <head>
  <title>STORES CARD</title>
  <link rel="stylesheet" href="<%templates%>/<%language_code%>/sqledger.css" />
</head>
<body>
  <table width="100%">

    <%include letterhead.html%>

    <tr>
        

       
        <h4>S T O R E S &nbsp;&nbsp; C A R D</h4>
       
    </tr>

    <tr>
        

       
        <table width="100%" cellspacing="0" cellpadding="0">
          <tr valign="top">
             
              <table>
                <tr>
                   Employee 
                   <%employee%> 
                </tr>
                <tr>
                   ID 
                   <%employee_id%> 
                </tr>
              </table>
             

             
              <table>
                <tr>
                   Card ID 
                   <%id%> 
                </tr>
                <tr>
                   Date 
                   <%transdate%> 
                </tr>
              </table>
             
          </tr>
        </table>
       
    </tr>

    <tr height="5">
        
    </tr>

    <tr>
        

       
        <table width="100%">
          <tr>
             
              <table width="100%">
                <tr>
                   Job # 
                   Description 
                </tr>

                <tr>
                   <%projectnumber%> 
                   <%projectdescription%> 
                </tr>
              </table>
             
          </tr>

          <tr height="15">
              
          </tr>

          <tr>
             
              <table width="100%" border="1">
                <tr>
                   Part Number 
                   Description 
                   Qty 
                   Cost 
                   Total 
                </tr>

                <tr>
                   <%partnumber%> 
                   <%description%> 
                   <%qty%> 
                   <%sellprice%> 
                   <%total%> 
                </tr>
              </table>
             
          </tr>
        </table>
       
    </tr>

<%if notes%>
    <tr height="5">
        
    </tr>

    <tr>
        
       <%notes%> 
    </tr>
<%end notes%>
  </table>
</body>
</html>
