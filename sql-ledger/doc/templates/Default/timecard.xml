<?xml version="1.0" encoding="utf-8"?>
<html>
<head>
  <title></title>
</head>
<body>
  <sql-ledger-root></sql-ledger-root>
  <head>
  <title>TIME CARD</title>
  <link rel="stylesheet" href="<%templates%>/<%language_code%>/sqledger.css" />
</head>
<body>
  <table width="100%">

    <%include letterhead.html%>

    <tr>
        

       
        <h4>T I M E &nbsp;&nbsp; C A R D</h4>
       
    </tr>

    <tr>
        

       
        <table width="100%" cellspacing="0" cellpadding="0">
          <tr valign="top">
             
              <table>
                <tr>
                   Employee 
                   <%employee%> 
                </tr>

                <tr>
                   ID 
                   <%employee_id%> 
                </tr>
              </table>
             

             
              <table>
                <tr>
                   Card ID 
                   <%id%> 
                </tr>

                <tr>
                   Date 
                   <%transdate%> 
                </tr>

                <tr>
                   In 
                   <%checkedin%> 
                </tr>

                <tr>
                   Out 
                   <%checkedout%> 
                </tr>

                <tr>
                   Hours 
                   <%qty%> 
                </tr>
              </table>
             
          </tr>
        </table>
       
    </tr>

    <tr height="5">
        
    </tr>

    <tr>
        

       
        <table width="100%">
          <tr valign="bottom">
             
              <table>
                <tr valign="top">
                   Job/Project # 
                   <%projectnumber%> 
                </tr>

                <tr>
                   Description 
                   <%projectdescription%> 
                </tr>

                <tr valign="top">
                   Labor/Service Code 
                   <%partnumber%> 
                </tr>

                <tr>
                   Description 
                   <%description%> 
                </tr>
              </table>
             

             
              <table>
                <tr>
                   Rate 
                   <%sellprice%> 
                </tr>

                <tr>
                   Total 
                   <%total%> 
                </tr>
              </table>
             
          </tr>
        </table>
       
    </tr>

<%if notes%>
    <tr height="5">
        
    </tr>

    <tr>
        

       <%notes%> 
    </tr>
<%end notes%>
  </table>
</body>
</html>
